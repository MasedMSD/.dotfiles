{
	"workbench.sideBar.location": "right",
	"workbench.colorTheme": "Ayu Dark Bordered",
	"workbench.iconTheme": "eq-material-theme-icons-darker",
	"workbench.activityBar.location": "top",
	"workbench.editor.showTabs": "multiple",
	"workbench.tree.enableStickyScroll": true,
	"terminal.integrated.gpuAcceleration": "off",
	"terminal.integrated.mouseWheelZoom": true,
	"git.openRepositoryInParentFolders": "always",
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	"window.zoomLevel": 1.2,
	"window.titleBarStyle": "custom",
	"window.menuBarVisibility": "compact",
	"editor.fontFamily": "Consolas, 'CaskaydiaMono Nerd Font Mono'",
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit"
	},
	"jsdoc-generator.includeDate": false,
	"jsdoc-generator.includeTime": false,
	"discord.removeRemoteRepository": true,
	"discord.detailsEditing": "ðŸ“ƒâ”†{file_name}",
	"discord.lowerDetailsEditing": "ðŸ“‚â”†{workspace}",
	"typescript.updateImportsOnFileMove.enabled": "never",
	"files.exclude": {
		"**/node_modules": true,
		"**/bun.lockb": true,
	},
	"redhat.telemetry.enabled": false,
	"security.workspace.trust.untrustedFiles": "open",
	"biome_lsp.trace.server": "messages",
	"[dockerfile]": {
		"editor.defaultFormatter": "ms-azuretools.vscode-docker"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
}